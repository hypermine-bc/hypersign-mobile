{
  "@quasar/testing-unit-jest": {
    "babel": "babelrc",
    "options": [
      "scripts",
      "SFC",
      "majestic"
    ]
  },
  "@quasar/testing-e2e-cypress": {
    "options": [
      "scripts"
    ]
  },
  "@quasar/testing-quality": {
    "options": [
      "scripts"
    ]
  },
  "@quasar/testing": {
    "harnesses": [
      "unit-jest",
      "e2e-cypress",
      "quality",
      "security"
    ]
  },
  "@quasar/dotenv": {
    "env_development": ".env.dev",
    "env_production": ".env.prod",
    "common_root_object": "hyptersignConfig",
    "create_env_files": true,
    "add_env_to_gitignore": true
  },
  "@quasar/qenv": {
    "common_root_object": "hypreSignQConfig",
    "add_qenv_to_gitignore": true
  }
}